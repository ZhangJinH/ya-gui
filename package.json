{
  "name": "ya-gui",
  "version": "0.0.1-alpha.0",
  "description": "A GUI tool for ya framework",
  "main": "src/index.js",
  "scripts": {
    "dist": "build --tasks win-x64,mac-x64 --mirror https://dl.nwjs.io/ ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q13/ya-gui.git"
  },
  "dependencies": {
    "antd": "3.9.2",
    "fs-extra": "7.0.0",
    "gaze": "1.1.3",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "terminate": "2.1.0",
    "ya-driver": "^0.0.1-alpha.0"
  },
  "keywords": [
    "GUI",
    "ya"
  ],
  "author": "13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/q13/ya-gui/issues"
  },
  "homepage": "https://github.com/q13/ya-gui#readme",
  "chromium-args": "--mixed-context",
  "window": {
    "icon": "src/deps/13.png"
  },
  "build": {
    "nwVersion": "0.33.3",
    "nwFlavor": "sdk"
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.14.8"
  },
  "webview": {
    "partitions": [
      {
        "name": "trusted",
        "accessible_resources": [
          "<all_urls>"
        ]
      }
    ]
  }
}
